function showktmBikelist(tabElement){$('#naked-bike, #supersport-bike, #travel-bike, #motocross-bike, #enduro-bike').removeClass("current");$('.tab1, .tab2, .tab3, .tab4, .tab5').hide();const tab=tabElement.dataset.tab;if(tab==="tab1"){$('.tab1').show()}else if(tab==="tab2"){$('.tab2').show()}else if(tab==="tab3"){$('.tab3').show()}else if(tab==="tab4"){$('.tab4').show()}else if(tab==="tab5"){$('.tab5').show()}
$(tabElement).addClass("current")}
$(document).ready(function(){$('.tab1').show();$('#naked-bike').addClass("current");$("#naked-bike").click(function(){window.location.href=$(this).attr('href')});$("#supersport-bike").click(function(){window.location.href=$(this).attr('href')});$("#travel-bike").click(function(){window.location.href=$(this).attr('href')});$("#motocross-bike").click(function(){window.location.href=$(this).attr('href')});$("#enduro-bike").click(function(){window.location.href=$(this).attr('href')})})